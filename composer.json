{
    "name": "fxp/block",
    "type": "library",
    "description": "Provides tools for defining UI blocks, rendering and mapping response data to related models",
    "keywords": ["block", "user interface", "ui"],
    "homepage": "https://github.com/fxpio/fxp-block",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.0",
        "symfony/options-resolver": "^3.4.0|^4.0.0",
        "symfony/event-dispatcher": "^3.4.0|^4.0.0",
        "symfony/form": "^3.4.0|^4.0.0"
    },
    "require-dev": {
        "symfony/dependency-injection": "^3.4.0|^4.0.0",
        "symfony/translation": "^3.4.0|^4.0.0",
        "symfony/templating": "^3.4.0|^4.0.0",
        "symfony/http-kernel": "^3.4.0|^4.0.0",
        "symfony/validator": "^3.4.0|^4.0.0",
        "symfony/doctrine-bridge": "^3.4.0|^4.0.0",
        "doctrine/orm": "^2.6.0",
        "twig/twig": "^2.4.0"
    },
    "suggest": {
        "doctrine/orm": "For doctrine block type"
    },
    "autoload": {
        "psr-4": { "Fxp\\Component\\Block\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
